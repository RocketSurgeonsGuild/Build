  <Import Project="$(MSBuildThisFileDirectory)\sourcelink.props" />
  <Import Project="$(MSBuildThisFileDirectory)\gitversion.props" />
  <Import Project="$(MSBuildThisFileDirectory)\gitlab.props" />
  <Import Project="$(MSBuildThisFileDirectory)\appveyor.props" />
