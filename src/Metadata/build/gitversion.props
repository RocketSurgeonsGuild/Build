<Project>
    <PropertyGroup>
        <CoreCompileDependsOn>
            $(CoreCompileDependsOn);
            AddGitVersionAttributes;
        </CoreCompileDependsOn>

        <RepositoryType>git</RepositoryType>
        <PackageVersion>$(GitVersion_NuGetVersion)</PackageVersion>
        <AssemblyVersion Condition="'$(GitVersion_Major)'!=''">$(GitVersion_Major).0.0.0</AssemblyVersion>
        <FileVersion>$(GitVersion_AssemblySemVer)</FileVersion>
        <InformationalVersion>$(GitVersion_InformationalVersion)</InformationalVersion>
    </PropertyGroup>
</Project>
