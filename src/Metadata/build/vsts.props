<Project>
    <PropertyGroup Condition="'$(TF_BUILD)'!=''">
        <CoreCompileDependsOn>
            $(CoreCompileDependsOn);
            AddVstsAttributes;
        </CoreCompileDependsOn>

        <PackageProjectUrl Condition="'$(BUILD_REPOSITORY_PROVIDER)' == 'GitHub'">$(BUILD_REPOSITORY_URI)</PackageProjectUrl>
        <ContinuousIntegrationBuild Condition="'$(ContinuousIntegrationBuild)' == '' and '$(DesignTimeBuild)' != 'true'">true</ContinuousIntegrationBuild>
    </PropertyGroup>
</Project>
